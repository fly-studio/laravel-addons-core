{
    "name": "addons/core",
    "description": "Laravel's Addons.",
    "homepage": "https://www.load-page.com/manuals",
    "keywords": ["Laravel","Addons"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/fly-studio/larvel-addons-core/issues",
        "source": "https://github.com/fly-studio/larvel-addons-core"
    },
    "authors": [
        {
            "name": "Fly",
            "email": "fly@load-page.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^9.0",
        "illuminate/translation": "^9.0",
        "illuminate/database": "^9.0",
        "addons/smarty": "~1.0",
        "addons/func": "~1.0",
        "addons/censor": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Addons\\Core\\": "src/"
        },
        "classmap": [
            "src/"
        ],
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Addons\\Core\\ServiceProvider"
            ],
            "aliases": {
            }
        }
    },
    "minimum-stability": "dev"

}
